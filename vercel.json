{
  "routes": [
    { "handle": "filesystem" },
    {
      "src": "/themes/(.*).(js.map|js|css|txt|html|png|json)",
      "dest": "/themes/$1",
      "headers": {
        "cache-control": "no-cache"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "headers": {
        "Cache-Control": "no-cache"
      }
    }
  ]
}
